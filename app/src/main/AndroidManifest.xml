<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="system.faisalshakiba.com.hellodoctor">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity"></activity>
        <activity android:name=".SecondActivity" />
        <activity android:name=".LandingActivity" />
        <activity android:name=".DoctorRegistration" />
        <activity android:name=".ThirdActivity" />
        <activity android:name=".DoctorActivity" />
        <activity android:name=".AppoinmentActivity" />
        <activity
            android:name=".PatientRegistration"
            android:label="@string/title_activity_patient_registration"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".CreateTimeActivity"
            android:label="@string/title_activity_create_time"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity android:name=".SplashScreen">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".PatientNavActivity"
            android:label="@string/title_activity_patient_nav"
            android:theme="@style/AppTheme.NoActionBar"></activity>
    </application>

</manifest>